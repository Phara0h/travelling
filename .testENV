
DATABASE_URL=postgres://postgres@localhost/travelling
NODE_TLS_REJECT_UNAUTHORIZED=0


## Server Settings
TRAVELLING_KEY=./localhost.key
TRAVELLING_CERT=./localhost.csr
TRAVELLING_PORT=6969
TRAVELLING_HTTPS=true

## Session Cookie Settings
TRAVELLING_COOKIE_SESSION_SECRET=notsosecret
TRAVELLING_COOKIE_SESSION_EXPIRATION=4

## Token Settings
TRAVELLING_COOKIE_TOKEN_SECRET=notsosecret2
TRAVELLING_COOKIE_TOKEN_SALT=saltytears
TRAVELLING_COOKIE_TOKEN_EXPIRATION=30

## Postgres Crypto Settings
TRAVELLING_PG_CRYPTO_IMPLEMENTATION_SECRET=notsosecret3
TRAVELLING_PG_CRYPTO_IMPLEMENTATION_SALT=saltiesttears


## Logging
TRAVELLING_LOG_FASTIFY_LOGGER=false
TRAVELLING_LOG_ENABLE=true

## Email
TRAVELLING_EMAIL_TEST_ENABLE=true
