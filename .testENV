
TRAVELLING_DATABASE_URL=postgres://postgres@localhost/travelling-test
NODE_TLS_REJECT_UNAUTHORIZED=0

## Server Settings
TRAVELLING_KEY=./localhost.key
TRAVELLING_CERT=./localhost.csr
TRAVELLING_PORT=6969
TRAVELLING_HTTPS=false

## Redis Settings
TRAVELLING_REDIS_ENABLE=false
TRAVELLING_REDIS_URL=redis://127.0.0.1:6379/2
TRAVELLING_REDIS_EVENTS_URL=redis://127.0.0.1:6379/3

## Session Cookie Settings
TRAVELLING_COOKIE_SESSION_SECRET=notsosecret
TRAVELLING_COOKIE_SESSION_EXPIRATION=4

## Token Settings
TRAVELLING_COOKIE_TOKEN_SECRET=notsosecret2
TRAVELLING_COOKIE_TOKEN_SALT=saltytears
TRAVELLING_COOKIE_TOKEN_EXPIRATION=30

## Postgres Crypto Settings
TRAVELLING_PG_CRYPTO_IMPLEMENTATION_SECRET=notsosecret3
TRAVELLING_PG_CRYPTO_IMPLEMENTATION_SALT=saltiesttears

## Logging
TRAVELLING_LOG_FASTIFY_LOGGER=false
TRAVELLING_LOG_REQUESTS=true
TRAVELLING_LOG_UNAUTHORIZED_ACCESS=true
TRAVELLING_LOG_LEVEL=trace

## Auditing
TRAVELLING_AUDIT_CREATE_ENABLE=true
TRAVELLING_AUDIT_EDIT_ENABLE=true
TRAVELLING_AUDIT_DELETE_ENABLE=true

## Email
TRAVELLING_EMAIL_TEST_ENABLE=false
